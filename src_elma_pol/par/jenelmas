C
      PARAMETER (NZOPER=11, NZOPQU=11, NZPARM=20, NZKMAX=3200,
     *          NZUMAX=600, NZBMAX=2200,  NDIML=350, NDIM=3680, 
     *          NDIM2=50, NZFMAX=80, NZRHOM=69, NDIMR=2500, NOUT=6)
C
C     BEI AENDERUNG VON NDIM1 MUSS AUCH NDIM1 IN EXODUS AUF DEN GLEICHEN WERT
C     GESETZT WERDEN!!!!!!!!!!!!!!!!!!
C
C     NZOPER: ANZAHL DER OPERATOREN IN ENEMB
C     NZOPQU: ANZAHL DER OPERATOREN IN QUAL
C     NZPARM: MAXIMALE ANZAHL RADIALWEITEN EINES BASISVEKTORS
C     NZKMAX:    "       "    DER KANAELE
C     NZUMAX:    "       "     "  UEBERLAGERUNGSKOEFFIZIENTEN
C     NZBMAX:    "       "    BASISVEKTOREN
C     NDIM  :    "      SUMME UEBER ALLE KANAELE (SUMME DER GERECHNETEN
C             RADIALPARAMETER DES KANALS
C             NDIM.LE.NZPARM*NZKMAX (HOECHSTMOEGLICHE ZAHL FUER NDIM)
C     NDIM1 : MAXIMALE ANZAHL VERSCHIEDENER EXPONENTIALVORFAKTOREN / KANAL
C     NDIM2 :    "       "          "       K-POTENZEN / KANAL
C     NZFMAX:    "       "    DER ZERLEGUNGEN
C
